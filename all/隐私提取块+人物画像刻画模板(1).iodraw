        <mxCell id="AYHAFoeZIL9HBaUtpC7M-2" value="&lt;font face=&quot;Lucida Console&quot;&gt;&lt;b&gt;&lt;i&gt;diary&lt;/i&gt;&lt;/b&gt;&lt;/font&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;shadow=1;" parent="1" vertex="1">
